/* pages/prizes/prizes.wxss */

.container {
  border-top: 1rpx solid #f0f0f0;
}

.add {
  height: 170rpx;
  padding: 30rpx;
}

.add view {
  width: 100%;
  height: 110rpx;
  border: 1rpx dotted #81c28d;
}

.frame>image {
  height: 460rpx;
  width: 100%;
  z-index: 1;
}

.frame .describe {
  width: 100%;
  position: absolute;
  bottom: 20rpx;
  left: 30rpx;
  font-size: 26rpx;
  color: #000;
}

.describe .name {
  font-size: 30rpx;
  margin-bottom: 15rpx;
}

.prizes image {
  height: 27rpx;
  width: 27rpx;
  padding-top: 9rpx;
}

.prizes view {
  padding-left: 28rpx;
  width: 625rpx;
  overflow: hidden;
  font-size: 28rpx;
  line-height: 46rpx;
}

.list {
  margin-bottom: 40rpx;
}

.list>view {
  margin-left: 20rpx;
  margin-top: 30rpx;
}
